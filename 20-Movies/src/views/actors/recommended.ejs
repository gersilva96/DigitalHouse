<!DOCTYPE html>
<html lang="es">
<head>
    <%- include("../partials/head", {title}) %>
</head>
<body>
    <%- include("../partials/header", {title, actorsSearch: true}) %>
    <div class="container">
        <ul>
            <% actors.forEach(actor => { %>
                <li><a href="/actors/detail/<%= actor.id %>"><%= `${actor.first_name} ${actor.last_name}` %></a></li>
            <% }); %>
        </ul>
    </div>
    <%- include("../partials/scripts") %>
</body>
</html>